load(AWS) 
c := aws:sqs:client(nil, "abc", "def")
io:disp("Created AWS SQS client ", c, "\n")
aws:sqs:create(c, "queue1")
io:disp("Created queue queue1\n")
aws:sqs:send(c, "queue1", "message1")
io:disp("Sent message1 to queue1\n")
message := aws:sqs:receive(c, "queue1")
io:disp("Received message ", message, "\n")
receiptHandle := term:attrib(message)
io:disp("Receipt handle = ")
io:show(receiptHandle)
io:disp("\n")
aws:sqs:delete(c, "queue1", receiptHandle)
io:disp("Removed message\n")
